{
  "name": "playground-native",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "playgrounds/playground-native/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nx/react-native:start",
      "dependsOn": ["sync-deps"],
      "options": {
        "port": 8081
      }
    },
    "run-ios": {
      "executor": "@nx/react-native:run-ios",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "bundle-ios": {
      "executor": "@nx/react-native:bundle",
      "dependsOn": [],
      "outputs": ["{options.bundleOutput}"],
      "options": {
        "entryFile": "src/main.tsx",
        "platform": "ios",
        "bundleOutput": "dist/playgrounds/playground-native/ios/main.jsbundle"
      }
    },
    "run-android": {
      "executor": "@nx/react-native:run-android",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "build-android": {
      "executor": "@nx/react-native:build-android",
      "outputs": [
        "{projectRoot}/android/app/build/outputs/bundle",
        "{projectRoot}/android/app/build/outputs/apk"
      ],
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "build-ios": {
      "executor": "@nx/react-native:build-ios",
      "outputs": ["{projectRoot}/ios/build/Build"],
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "pod-install": {
      "executor": "@nx/react-native:pod-install",
      "options": {}
    },
    "bundle-android": {
      "executor": "@nx/react-native:bundle",
      "dependsOn": [],
      "outputs": ["{options.bundleOutput}"],
      "options": {
        "entryFile": "src/main.tsx",
        "platform": "android",
        "bundleOutput": "dist/playgrounds/playground-native/android/main.jsbundle"
      }
    },
    "sync-deps": {
      "executor": "@nx/react-native:sync-deps",
      "options": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "playgrounds/playground-native/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/playgrounds/playground-native/web",
        "index": "playgrounds/playground-native/src/index.html",
        "baseHref": "/",
        "main": "playgrounds/playground-native/src/main-web.tsx",
        "tsConfig": "playgrounds/playground-native/tsconfig.app.json",
        "assets": [
          "playgrounds/playground-native/src/favicon.ico",
          "playgrounds/playground-native/src/assets"
        ],
        "webpackConfig": "playgrounds/playground-native/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "playground-native:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "playground-native:build:development"
        },
        "production": {
          "buildTarget": "playground-native:build:production",
          "hmr": false
        }
      }
    },
    "upgrade": {
      "executor": "@nx/react-native:upgrade",
      "options": {}
    }
  },
  "tags": []
}
